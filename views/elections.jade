extend layout
block content
  .page-header
    h1= title 
  table.table.elections
    thead
      tr
        th Title
        th Tokens
        th Start Time
        th End Time
    tbody
      each election in elections
        tr
          td
            a(href="/admin/election/" + election.slug)= election.title
          td= election.tokens
          td= election.startTime
          td= election.endtime


